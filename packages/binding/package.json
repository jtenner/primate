{
  "name": "@primate/binding",
  "version": "0.7.1",
  "description": "Primate binding module",
  "homepage": "https://primatejs.com/modules/binding",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.go",
    "src/**/*.py",
    "src/**/*.rb",
    "src/**/*.sum",
    "src/**/*.mod",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/binding"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/build": "^0.3.0",
    "@rcompat/cli": "^0.5.0",
    "@rcompat/env": "^0.3.0",
    "@rcompat/fs": "^0.4.0",
    "@rcompat/object": "^0.5.0",
    "@rcompat/platform": "^0.3.0",
    "@rcompat/stdio": "^0.4.0"
  },
  "peerDependencies": {
    "@ruby/head-wasm-wasi": "2",
    "@ruby/wasm-wasi": "2",
    "primate": "workspace:^",
    "pyodide": "0.26"
  },
  "peerDependenciesMeta": {
    "@ruby/head-wasm-wasi": {
      "optional": true
    },
    "@ruby/wasm-wasi": {
      "optional": true
    },
    "pyodide": {
      "optional": true
    }
  },
  "type": "module",
  "imports": {
    "#*": {
      "@primate/lt": "./src/private/*.js",
      "default": "./src/private/*.js"
    },
    "#error/*": {
      "@primate/lt": "./src/private/error/*.js",
      "default": "./src/private/error/*.js"
    }
  },
  "exports": {
    "./*/common": "./src/server/*/common.js",
    "./*/to-request": "./src/server/*/to-request.js",
    "./*/to-response": "./src/server/*/to-response.js",
    "./*/wrap": "./src/server/*/wrap.js",
    "./*": {
      "runtime": "./src/server/*/runtime.js",
      "default": "./src/server/*/default.js"
    }
  }
}
