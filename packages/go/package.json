{
  "name": "@primate/go",
  "version": "0.1.6",
  "description": "Go backend for Primate",
  "homepage": "https://primate.run/backend/go",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.go",
    "/lib/**/*.sum",
    "/lib/**/*.mod",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/go"
  },
  "scripts": {
    "build": "npm run clean && tsc && cp -a src/private/*.go lib/private && cp src/private/go.* lib/private",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.7.0",
    "@rcompat/env": "^0.6.1",
    "@rcompat/fs": "^0.11.5",
    "@rcompat/invariant": "^0.7.0",
    "@rcompat/runtime": "^0.3.0",
    "@rcompat/stdio": "^0.8.0",
    "@rcompat/string": "^0.6.1"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#error/*": {
      "apekit": "./src/private/error/*.ts",
      "default": "./lib/private/error/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/runtime.js",
      "default": "./lib/default.js"
    },
    "./*": "./lib/*.js"
  },
  "devDependencies": {
    "@rcompat/record": "^0.6.2"
  }
}
