{
  "name": "@primate/go",
  "version": "0.1.0",
  "description": "Primate Go module",
  "homepage": "https://primatejs.com/modules/go",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.go",
    "src/**/*.sum",
    "src/**/*.mod",
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/go"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.5.1",
    "@rcompat/env": "^0.3.0",
    "@rcompat/fs": "^0.4.0",
    "@rcompat/invariant": "^0.5.0",
    "@rcompat/object": "^0.5.0",
    "@rcompat/platform": "^0.3.0",
    "@rcompat/stdio": "^0.4.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "@primate/lt": "./src/private/*.js",
      "default": "./src/private/*.js"
    },
    "#error/*": {
      "@primate/lt": "./src/private/error/*.js",
      "default": "./src/private/error/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./src/runtime.js",
      "default": "./src/default.js"
    },
    "./*": "./src/*.js"
  }
}
