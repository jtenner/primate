{
  "name": "@primate/core",
  "version": "0.1.10",
  "description": "Primate core",
  "homepage": "https://primatejs.com",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js",
    "src/build/defaults/*.html"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/core"
  },
  "dependencies": {
    "@rcompat/args": "^0.3.0",
    "@rcompat/array": "^0.3.0",
    "@rcompat/async": "^0.4.0",
    "@rcompat/build": "^0.6.0",
    "@rcompat/cli": "^0.5.1",
    "@rcompat/crypto": "^0.5.0",
    "@rcompat/fs": "^0.7.0",
    "@rcompat/function": "^0.4.0",
    "@rcompat/http": "^0.6.1",
    "@rcompat/invariant": "^0.5.0",
    "@rcompat/object": "^0.5.0",
    "@rcompat/package": "^0.8.0",
    "@rcompat/runtime": "^0.1.0",
    "@rcompat/stdio": "^0.6.0",
    "@rcompat/sync": "^0.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.3"
  },
  "type": "module",
  "imports": {
    "#*": {
      "livetypes": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#error/*": {
      "livetypes": "./src/private/error/*.ts",
      "default": "./lib/private/error/*.js"
    }
  },
  "exports": {
    "./serve": "./lib/serve/index.js",
    "./build": "./lib/build/index.js",
    "./error": "./lib/error/index.js",
    "./log": "./lib/log/index.js",
    "./dispatch": "./lib/dispatch/index.js",
    "./http/verbs": "./lib/http/verbs.js",
    "./handler/*": "./lib/serve/handler/*.js"
  }
}
