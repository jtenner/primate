{
  "name": "primate",
  "version": "0.33.0-pre",
  "description": "The universal web framework",
  "homepage": "https://primatejs.com",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "bin": "lib/bin.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/primate"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/args": "^0.5.0",
    "@rcompat/async": "^0.6.0",
    "@rcompat/cli": "^0.7.0",
    "@rcompat/fs": "^0.11.5",
    "@rcompat/http": "~0.10.0",
    "@rcompat/package": "^0.10.1"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "imports": {
    "#loader": "@primate/core/serve/loader"
  },
  "exports": {
    "./route": {
      "apekit": "./src/public/route.ts",
      "default": "./lib/public/route.js"
    },
    "./handler/*": {
      "apekit": "./src/handlers/*.ts",
      "default": "./lib/handlers/*.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    },
    "./runtime/*": {
      "apekit": "./lib/runtime/*.ts",
      "default": "./lib/runtime/*.js"
    }
  }
}
