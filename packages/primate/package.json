{
  "name": "primate",
  "version": "0.33.0-pre",
  "description": "The Universal Web Framework",
  "homepage": "https://primatejs.com",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js",
    "types/*.ts"
  ],
  "bin": "src/bin.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/primate"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/args": "^0.4.0",
    "@rcompat/async": "^0.5.1",
    "@rcompat/cli": "^0.6.0",
    "@rcompat/fs": "^0.9.7",
    "@rcompat/http": "~0.9.1",
    "@rcompat/package": "^0.9.0"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "exports": {
    "./handler/*": {
      "types": "./types/index.d.ts",
      "default": "./src/handlers/*.js"
    },
    "./*": "./src/public/*.js",
    "./runtime/*": "./src/runtime/*.js"
  }
}
