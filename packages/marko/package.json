{
  "name": "@primate/marko",
  "version": "0.1.4",
  "description": "Primate Marko frontend",
  "homepage": "https://primatejs.com/modules/marko",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/marko"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@marko/compiler": "^5.39.13",
    "@marko/translator-default": "^6.1.3",
    "@primate/core": "workspace:^",
    "marko": "^5.37.20"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#build": {
      "apekit": "./src/private/build/index.ts",
      "default": "./lib/private/build/index.js"
    },
    "#serve": {
      "apekit": "./src/private/serve/index.ts",
      "default": "./lib/private/serve/index.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    }
  },
  "devDependencies": {
    "@types/marko": "^4.6.8"
  }
}
