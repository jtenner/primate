{
  "name": "@primate/vue",
  "version": "0.11.0",
  "description": "Vue frontend",
  "homepage": "https://primatejs.com/modules/vue",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/vue"
  },
  "dependencies": {
    "@primate/frontend": "workspace:^",
    "@rcompat/build": "^0.10.1"
  },
  "peerDependencies": {
    "primate": "workspace:^",
    "vue": "3"
  },
  "type": "module",
  "imports": {
    "#*": {
      "livetypes": "./src/private/*.js",
      "default": "./src/private/*.js"
    },
    "#build": {
      "livetypes": "./src/private/build/index.js",
      "default": "./src/private/build/index.js"
    },
    "#serve": {
      "livetypes": "./src/private/serve/index.js",
      "default": "./src/private/serve/index.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./src/public/runtime.js",
      "default": "./src/public/default.js"
    }
  }
}
