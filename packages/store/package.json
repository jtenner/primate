{
  "name": "@primate/store",
  "version": "0.24.0",
  "description": "Primate data store module",
  "homepage": "https://primatejs.com/modules/store",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "src/errors.json",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/store"
  },
  "scripts": {
    "db:surreal": "surreal start --user primate --pass primate"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/array": "^0.2.0",
    "@rcompat/async": "^0.2.0",
    "@rcompat/cli": "^0.2.0",
    "@rcompat/fs": "^0.3.0",
    "@rcompat/function": "^0.3.0",
    "@rcompat/invariant": "^0.4.0",
    "@rcompat/object": "^0.4.0",
    "@rcompat/platform": "^0.2.0",
    "@rcompat/sql": "^0.3.0",
    "@rcompat/sync": "^0.2.0"
  },
  "devDependencies": {
    "@primate/types": "workspace:^",
    "better-sqlite3": "^11.1.2",
    "mongodb": "^6.8.0",
    "mysql2": "^3.11.0",
    "postgres": "^3.4.4",
    "surrealdb.js": "^0.11.1"
  },
  "peerDependencies": {
    "better-sqlite3": "11",
    "mongodb": "6",
    "mysql2": "3",
    "postgres": "3",
    "primate": "workspace:^",
    "surrealdb.js": "0.11"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "surrealdb.js": {
      "optional": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "runtime": "./src/runtime.js",
      "default": "./src/default.js"
    },
    "./*/common": "./src/server/*/common.js",
    "./*/hooks/serve": "./src/server/*/hooks/serve/index.js",
    "./base/*": "./src/base/*.js",
    "./hooks/*": "./src/hooks/*.js",
    "./sql/*": "./src/sql/*.js",
    "./errors/*": "./src/errors/*.js",
    "./*": "./src/server/*/default.js"
  }
}
