{
  "name": "@primate/ruby",
  "version": "0.1.4",
  "description": "Ruby backend",
  "homepage": "https://primatejs.com/modules/ruby",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.rb",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/ruby"
  },
  "scripts": {
    "build": "npm run clean && tsc && cp -a src/private/*.rb lib/private",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rcompat/record": "^0.6.2"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/fs": "^0.11.5",
    "@rcompat/package": "^0.10.1",
    "@ruby/head-wasm-wasi": "^2.7.1",
    "@ruby/wasm-wasi": "^2.7.1"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/runtime.js",
      "default": "./lib/default.js"
    },
    "./*": "./lib/*.js"
  }
}
