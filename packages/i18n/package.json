{
  "name": "@primate/i18n",
  "version": "0.8.3",
  "description": "Primate internalization",
  "homepage": "https://primatejs.com/modules/i18n",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/i18n"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.7.0",
    "@rcompat/fs": "^0.11.5",
    "@rcompat/http": "^0.10.0",
    "@rcompat/record": "^0.6.0"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#hook/*": {
      "apekit": "./src/private/hook/*.ts",
      "default": "./lib/private/hook/*.js"
    },
    "#error/*": {
      "apekit": "./src/private/error/*.ts",
      "default": "./lib/private/error/*.js"
    }
  },
  "exports": {
    "./core/*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    },
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    }
  }
}
