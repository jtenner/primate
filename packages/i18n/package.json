{
  "name": "@primate/i18n",
  "version": "0.7.0",
  "description": "Primate I18N module",
  "homepage": "https://primatejs.com/modules/i18n",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "src/errors.json",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/i18n"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.5.1",
    "@rcompat/fs": "^0.4.0",
    "@rcompat/http": "^0.5.0"
  },
  "type": "module",
  "imports": {
    "#*": {
      "@primate/lt": "./src/private/*.js",
      "default": "./src/private/*.js"
    },
    "#hook/*": {
      "@primate/lt": "./src/private/hook/*.js",
      "default": "./src/private/hook/*.js"
    },
    "#error/*": {
      "@primate/lt": "./src/private/error/*.js",
      "default": "./src/private/error/*.js"
    }
  },
  "exports": {
    "./*/locale": "./src/server/*/locale.js",
    "./*": "./src/server/*/index.js",
    ".": {
      "runtime": "./src/runtime.js",
      "default": "./src/default.js"
    }
  }
}
