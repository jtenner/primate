{
  "name": "@primate/i18n",
  "version": "0.8.3",
  "description": "I18N for Primate",
  "homepage": "https://primate.run/docs/i18n",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/i18n"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.8.0",
    "@rcompat/fs": "^0.12.3",
    "@rcompat/http": "^0.11.0",
    "@rcompat/invariant": "^0.8.1"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#hook/*": {
      "apekit": "./src/private/hook/*.ts",
      "default": "./lib/private/hook/*.js"
    },
    "#error/*": {
      "apekit": "./src/private/error/*.ts",
      "default": "./lib/private/error/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
