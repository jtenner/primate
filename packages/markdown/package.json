{
  "name": "@primate/markdown",
  "version": "0.4.3",
  "description": "Markdown frontend",
  "homepage": "https://primatejs.com/modules/markdown",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/markdown"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/invariant": "^0.7.0",
    "marked": "^15.0.7"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#build": {
      "apekit": "./src/private/build/index.ts",
      "default": "./lib/private/build/index.js"
    },
    "#serve": {
      "apekit": "./src/private/serve/index.ts",
      "default": "./lib/private/serve/index.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    }
  }
}
