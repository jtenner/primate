{
  "name": "@primate/frontend",
  "version": "0.16.2",
  "description": "Primate frontend core",
  "homepage": "https://primatejs.com/modules/frontend",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/frontend"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/async": "^0.3.0",
    "@rcompat/build": "^0.4.0",
    "@rcompat/crypto": "^0.5.0",
    "@rcompat/fs": "^0.5.1",
    "@rcompat/http": "^0.5.3",
    "@rcompat/object": "^0.5.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "@primate/lt": "./src/private/*.js",
      "default": "./src/private/*.js"
    },
    "#error/*": {
      "@primate/lt": "./src/private/error/*.js",
      "default": "./src/private/error/*.js"
    },
    "#*/name": {
      "@primate/lt": "./src/server/*/name.js",
      "default": "./src/server/*/name.js"
    },
    "#*/extension": {
      "@primate/lt": "./src/server/*/extension.js",
      "default": "./src/server/*/extension.js"
    },
    "#*/handler": {
      "@primate/lt": "./src/server/*/handler.js",
      "default": "./src/server/*/handler.js"
    },
    "#*/build": {
      "@primate/lt": "./src/server/*/build/index.js",
      "default": "./src/server/*/build/index.js"
    },
    "#*/serve": {
      "@primate/lt": "./src/server/*/serve/index.js",
      "default": "./src/server/*/serve/index.js"
    }
  },
  "exports": {
    ".": {
      "browser": {
        "spa": "./src/spa/exports.js"
      }
    },
    "./spa": "./src/spa/exports.js",
    "./core/*": {
      "@primate/lt": "./src/public/*.js",
      "default": "./src/public/*.js"
    },
    "./*": {
      "runtime": "./src/server/*/public/runtime.js",
      "default": "./src/server/*/public/default.js"
    }
  }
}
