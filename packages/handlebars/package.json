{
  "name": "@primate/handlebars",
  "version": "0.1.3",
  "description": "Primate Handlebars frontend",
  "homepage": "https://primatejs.com/modules/handlebars",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/handlebars"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "handlebars": "^4.7.8"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#build": {
      "apekit": "./src/private/build/index.ts",
      "default": "./lib/private/build/index.js"
    },
    "#serve": {
      "apekit": "./src/private/serve/index.ts",
      "default": "./lib/private/serve/index.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    }
  }
}
